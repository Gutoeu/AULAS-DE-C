#include <stdio.h>

int main()
{
    int numero,quantidade;
    float porcentagem;
    printf("Digite um n√∫mero:");
    scanf("%d",&numero);
    
    printf("Quantidade:");
    scanf("%d",&quantidade);
    porcentagem = (numero*quantidade)/100;
    
    printf("Pocentagem: %.2f",porcentagem);
    return 0;
}

